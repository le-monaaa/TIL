{
    "manifest_version": 3,
    "name": "extension-test",
    "description": "백준 따라하기",
    "version": "1.1.22",
    "author": "flaxinger",
    "action": {
      "default_icon": "images/icon-16.png",
      "default_popup": "popup.html"
    },
    "icons": {
      "16": "images/icon-16.png",
      "48": "images/icon-16.png",
      "128": "images/icon-16.png"
    },
    "background": {
      "service_worker": "scripts/background.js"
    },
    "host_permissions": [
      "https://www.acmicpc.net/",
      "https://solved.ac/api/v3/*"
    ],
    "web_accessible_resources": [
      {
        "matches": [
          "<all_urls>"
        ],
        "resources": [
          "popup.html",
          "popup.js",
          "solved.js"
        ]
      }
    ],
    "content_scripts" : [
      {
        "matches" : [
          "https://www.acmicpc.net/status?*from_mine*"
        ],
        "js" : [
          "scripts/baekjoon/solved.js"
          
        ]
      },
      {
        "matches" : [
          "https://www.acmicpc.net/problem/*"
        ],
        "js" : [
          "scripts/baekjoon/problem.js"
        ]
      },
      {
        "matches" : [
          "https://www.acmicpc.net/source/*"
        ],
        "js" : [
          "scripts/baekjoon/submit.js"
        ] 
      },
      {
        "matches" : [
          "https://swexpertacademy.com/main/solvingProblem/solvingProblem.do"
        ],
        "js": [
          "scripts/swea/problem.js"
        ]
      }
    ]
  }
  